{

  "description": "Adds a Pomodoro Timer",
  "manifest_version": 2,
  "name": "QPomo",
  "version": "1.0",
  "icons": {
    "48": "icons/clock-48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/beasts-32.png",
    "default_title": "QPomo",
    "default_popup": "popup/timer.html"
  }


}