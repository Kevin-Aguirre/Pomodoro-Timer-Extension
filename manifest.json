
{
  "manifest_version":2,
  "name":"QPomo",
  "verison": 1.0,

  "description":"Adds a Pomodoro Timer to the toolbar",
  "homepage_url":"",

  "icons" : {
    "48":"icons/clock.png"
  },

  "permissions" : ["activeTab"],

  "browser_action": {
    "default_icon": "icons/clock.png",
    "default_title": "QPomo",
    "default_popup": "popup/timer.html"
  }  

}